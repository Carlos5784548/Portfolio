---
import TerminalIcon from '../../atoms/terminal-icon/TerminalIcon.astro';
import Terminal from '../terminal/Terminal.astro';
import './skills-on-terminal.css';
---

<Terminal nombre="CarlosTrod">
	<div class="terminal__skills">
		<h2>Mis habilidades</h2>
		<div class="terminal__skills__container">
			<TerminalIcon
				src="/assets/bootstrap.svg"
				color="#7738ff"
				name="Bootstrap"
			/>
			<TerminalIcon
				src="/assets/javascript.svg"
				color="#7738ff"
				name="Javascript"
			/>
			<TerminalIcon
				src="/assets/mysql.svg"
				color="#7738ff"
				name="Mysql"
			/>

			<TerminalIcon
				src="/assets/html.svg"
				color="#FF4747"
				name="html"
			/>
			<TerminalIcon
				src="/assets/icons8-java.svg"
				color="#00D1FF"
				name="Java"
			/>
			

			<TerminalIcon src="/assets/Astro.svg" color="#7738ff" name="Astro" />


			<TerminalIcon src="/assets/css.svg" color="#00A3FF" name="CSS" />
			<TerminalIcon
				src="/assets/wordpress.svg"
				color="#3F75FF"
				name="Wordpress"
			/>
		</div>
	</div>
</Terminal>
